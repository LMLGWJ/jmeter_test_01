	String[] paras = para.split(&quot;=&quot;);
	parameters.put(&quot;_mt&quot;, &quot;user.webLogin&quot;);
	parameters.put(&quot;_aid&quot;, &quot;1&quot;);
	parameters.put(&quot;_sm&quot;, &quot;md5&quot;);
